<table class="table  px-5" style="border-radius: 6px;">
    <thead>
        <tr>
            <th scope="col">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                <span class="heading ">Name</span>
            </th>
            <th scope="col"><span class="heading">Item Type</span></th>
            <th scope="col"><span class="heading">Rate</span></th>
            <th scope="col"><span class="heading">Quantity</span> </th>
            <th scope="col"><span class="heading">Available</span></th>
            <th scope="col"><span class="heading">Action</span></th>
        </tr>
    </thead>
    <tbody>
         @foreach (var item in Model)
         {
        <tr>
            <td class="d-flex">
                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                <img src="~/images/dining-menu.png" alt="profile image" class="me-2 bg-light" id="profile"
                    style="max-height:35px; height: 100%; width:auto; border-radius: 100%;">
                <span>@item.Itemname</span>
            </td>
            <td><img src="~/images/icons/non-veg-icon.svg" style="width: 20px;" class="ms-4"></td>
            <td>@item.Rate</td>
            <td>@item.Quantity</td>
            <td class="mx-auto">
                <div class="form-check form-switch">
                    <input class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" @(item.Isavailable ? "checked" : "")>
                </div>
            </td>
            <td>
                <a href="Edit_user.html">
                    <img src="~/images/pen.svg" alt="profile image" class="me-2 bg-light" id="profile"
                        style="max-height: 30px; height: 100%; width:auto;">
                </a>
                <a data-bs-toggle="modal" data-bs-target="#exampleModalCenter">
                    <img src="~/images/trash.svg" alt="profile image" class="me-2 bg-light" id="profile"
                        style="max-height: 30px; height: 100%; width:auto; border-radius: 100%;">
                </a>

            </td>
        </tr>
         }
    </tbody>
</table>